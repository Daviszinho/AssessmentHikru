<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="oracle.manageddataaccess.client" type="OracleInternal.Common.ODPMasmConfiguration, Oracle.ManagedDataAccess, Version=4.122.19.1, Culture=neutral, PublicKeyToken=89b483f429c47342" />
  </configSections>
  <oracle.manageddataaccess.client>
    <version number="*">
      <settings>
        <setting name="TNS_ADMIN" value="/home/site/wwwroot/wallet" />
        <setting name="WALLET_LOCATION" value="(SOURCE=(METHOD=FILE)(METHOD_DATA=(DIRECTORY=/home/site/wwwroot/wallet)))" />
        <setting name="SQLNET.AUTHENTICATION_SERVICES" value="(NONE)" />
        <setting name="SQLNET.AUTHENTICATION_KERBEROS5_CC_NAME" value="OSMSFT:" />
        <setting name="SQLNET.AUTHENTICATION_KERBEROS5_CONF" value="%ORACLE_HOME%\network\admin\krb5.conf" />
        <setting name="SQLNET.AUTHENTICATION_KERBEROS5_CONF_MIT" value="TRUE" />
        <setting name="SQLNET.KERBEROS5_CC_NAME" value="OSMSFT:" />
        <setting name="SQLNET.KERBEROS5_CONF" value="%ORACLE_HOME%\network\admin\krb5.conf" />
        <setting name="SQLNET.KERBEROS5_CONF_MIT" value="TRUE" />
        <setting name="SQLNET.KERBEROS5_REALMS" value="%ORACLE_HOME%\network\admin\krb.realms" />
      </settings>
      <dataSources>
        <dataSource alias="oracletest_high" descriptor="(description=(retry_count=20)(retry_delay=3)(address=(protocol=tcps)(port=1522)(host=adb.us-ashburn-1.oraclecloud.com))(connect_data=(service_name=oracletest_high))(security=(ssl_server_cert_dn="CN=adwc.uscom-east-1.oraclecloud.com, OU=Oracle ADWC, O=Oracle Corporation, L=Redwood City, ST=California, C=US")))" />
      </dataSources>
    </version>
  </oracle.manageddataaccess.client>
  <system.webServer>
    <handlers>
      <add name="aspNetCore" path="*" verb="*" modules="AspNetCoreModuleV2" resourceType="Unspecified" />
    </handlers>
    <aspNetCore processPath="dotnet" 
               arguments=".\RestWebServices.dll" 
               stdoutLogEnabled="true" 
               stdoutLogFile="\\?\%home%\LogFiles\stdout"
               hostingModel="inprocess">
      <environmentVariables>
        <environmentVariable name="ASPNETCORE_ENVIRONMENT" value="Development" />
        <environmentVariable name="ASPNETCORE_DETAILEDERRORS" value="true" />
      </environmentVariables>
    </aspNetCore>
    <httpErrors errorMode="Detailed" />
    <httpProtocol>
      <customHeaders>
        <remove name="X-Powered-By" />
      </customHeaders>
    </httpProtocol>
  </system.webServer>
  <system.web>
    <customErrors mode="Off" />
    <compilation debug="true" targetFramework="9.0" />
    <httpRuntime targetFramework="9.0" />
  </system.web>
  <location path="." inheritInChildApplications="false" />
</configuration>